referencing Pure

NativeOutputStream class: OutputStream {
	val identifier: Identifier

	init(identifier)
	
	native overriding to writeByte(byte: Byte)
	native overriding to writeBytes(bytes: ByteArray)

	overriding operator ==(right: Any?): Bool {
		if right is! rightNativeOutputStream: NativeOutputStream
			return no
		return identifier == rightNativeOutputStream.identifier
	}

	alias Identifier = Int {
		instances STANDARD_OUTPUT(1), STANDARD_ERROR(2)
	}
}
