referencing Pure

copied String class {
	computed byteCount: Int
		gets bytes.size
	computed characterCount: Int
		gets bytes.size
	var bytes: <Byte>Array

	init(bytes)

	operator +=(right: String) {
		bytes = bytes + right.bytes
	}
	operator +(right: String): String {
		return String(bytes + right.bytes)
	}
}
